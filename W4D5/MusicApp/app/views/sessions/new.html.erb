<h1>Log In</h1>

<form class="" action="<%= session_url %>" method="post">
  <%= render 'shared/forgery' %>

  <label for="name">Username:</label><br>
  <input type="text" name="session[username]" id="name" value=""><br>

  <label for="pass">Password:</label><br>
  <input type="password" name="session[password]" id="pass" value=""><br>

  <input type="submit" name="" value="Login!">
</form>
